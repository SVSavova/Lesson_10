/*
Направете програма PrintTriangle1, която печата на екрана следната фигура:
1 2 3 4 5
1 2 3 4
1 2 3
1 2
1
Нека размерът на фигурата е дефиниран от променлива int size = 5;
 */

public class task_Triangle {
    public static void main(String[] args) {

        int size = 5;
        for (int i = 0; i <= size; i++) {
            System.out.println();
            for (int j = 1; j <= size - i; j++) {
                System.out.print(j + " ");
            }
        }
    }
}

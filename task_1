/*
Напишете програма, която събира само положителни числа до 100, ако
потребителят въведе отрицателно число го пропускате.
 */

import java.util.Scanner;

public class task_sum {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
        int number = 0;
        int i = 0;
        int temp = 0;
        while (true) {
            System.out.print("Enter a positive number [1 to 100]: ");
            number = scanner.nextInt();
            if (number > 0) {
                break;
            }
        }
        for (i = number; i < 100; i++) {
            i++;
            System.out.println(i);
        }
    }
}
